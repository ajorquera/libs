{
  "name": "@ajorquera/libs",
  "private": false,
  "version": "1.0.1",
  "type": "module",
  "types": "./dist/components.d.ts",
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "stories": "yarn ladle serve",
    "build:react": "tsc -b && vite build -c ./viteconfigs/react.config.ts",
    "build:utils": "tsc -b && vite build -c ./viteconfigs/utils.config.ts",
    "build": "yarn build:react && yarn build:utils",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "yarn build"
  },
  "main": "./dist/components.umd.cjs",
  "module": "./dist/components.js",
  "exports": {
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.umd.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.umd.cjs"
    }
  },
  "peerDependencies": {
    "firebase": "^11.6.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "firebase": "^11.7.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@eslint/js": "^9.22.0",
    "@ladle/react": "^5.0.2",
    "@types/node": "^22.15.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.2",
    "vitest-browser-react": "^0.1.1"
  },
  "description": "React components that will help to build different projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajorquera/components.git"
  },
  "author": "Andres Jorquera",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ajorquera/components/issues"
  },
  "homepage": "https://github.com/ajorquera/components#readme",
  "dependencies": {}
}